extends layout

block content

  h1= title
  p Welcome to #{title}

  ul
  each group in groups
    section(class="card")
      div
        h1 #{group.get("name")}
      div
        if group.get("icon")
          a(href='/groups/' + group.id)
            img(src=group.get("icon")._url width="100" height="auto")
